
clc; close all; clear all;

%% Load and clean data
state = readmatrix('GAINS 8 Pos Test/temp/state.csv'); % Data from freezer test
raw = state(11000:145000,3);
clear state;

%% Fit a line to find slope
len = length(raw);
x = 1:len;
p = polyfit(x,raw,1);

%% Center noise about x-axis
shifted = raw' - p(1).*(x-mean(x));
shifted = shifted - mean(shifted);


figure;
plot(shifted);
